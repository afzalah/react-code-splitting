  "nyc": {
    "include": [
      "src/**/*.tsx",
      "src/**/*.ts"
    ],
    "extension": [
      ".tsx",
      ".ts"
    ],
    "exclude": [
      "src/**/*.test.tsx",
      "src/**/*.test.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "scripts": {
    "prepush": "npm run lint && npm run test",
    "debug": "npm run build-vendor && webpack-dev-server --config webpack.config.js --host 0.0.0.0 --port 52000 --hot --inline --history-api-fallback",
    "build": "npm run lint && npm run test && npm run clean && NODE_ENV=production npm run build-vendor && NODE_ENV=production npm run build-assets",
    "build-assets": "webpack --config webpack.config.js",
    "build-vendor": "webpack --config vendor.webpack.config.js",
    "clean": "rm -rf dist/**",
    "test": "mocha --recursive -r ts-node/register -r jsdom-global/register -r ignore-styles -r ./src/test-setup.ts --reporter min 'src/**/*.test.ts' 'src/**/*.test.tsx'",
    "lint": "npm run lint-ts && npm run lint-less",
    "lint-ts": "tslint -c tslint.json --project tsconfig.json --type-check 'src/**/*.{ts,tsx}'",
    "lint-less": "stylelint \"src/**/*.less\" --syntax less",
    "cover": "NODE_ENV=test nyc npm test"
  },
  "engines": {
    "npm": ">=2.0.0"
  },
  "private": true
